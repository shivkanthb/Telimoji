  "devDependencies": {
    "browserify": "^11.0.0",
    "eslint": "^1.0.0",
    "esmangle": "^1.0.0",
    "istanbul": "^0.3.0",
    "jscs": "^2.0.0",
    "jscs-jsdoc": "^1.0.0",
    "markdown-table": "^0.4.0",
    "mdast": "^1.0.0",
